{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
    'docker:enableMajor',
    "github>fma965/f9-homelab//.renovate/autoMerge.json5",
    "github>fma965/f9-homelab//.renovate/customManagers.json5",
    "github>fma965/f9-homelab//.renovate/grafanaDashboards.json5",
    "github>fma965/f9-homelab//.renovate/groups.json5",
    "github>fma965/f9-homelab//.renovate/labels.json5",
    "github>fma965/f9-homelab//.renovate/overrides.json5",
    "github>fma965/f9-homelab//.renovate/semanticCommits.json5",
    ":automergeBranch",
    ':disableRateLimiting',
    ":dependencyDashboard",
    ":semanticCommits",
    ':timezone(Europe/London)',
  ],
  dependencyDashboardTitle: 'Renovate Dashboard ðŸ¤–',
  suppressNotifications: [
    'prEditedNotification',
    'prIgnoreNotification',
  ],
  ignorePaths: [
    'infrastructure/talos/main.tf',
    'infrastructure/talos/talos/providers.tf',
    '.githooks/**',
    '.github/**',
    '.sops/**',
    '**/*.sample',
    '**/*.sops.*',
    "**/resources/**",
    "archive/**"
  ],
  flux: {
    managerFilePatterns: [
      '/(^|/)kubernetes/.+\\.ya?ml$/',
    ],
  },
  'helm-values': {
    managerFilePatterns: [
      '/(^|/)kubernetes/.+\\.ya?ml$/',
    ],
  },
  kubernetes: {
    managerFilePatterns: [
      '/(^|/)kubernetes/.+\\.ya?ml$/',
    ],
  },
}
